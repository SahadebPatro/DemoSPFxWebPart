    "ms-shop-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/msShop/MsShopWebPart.js",
          "manifest": "./src/webparts/msShop/MsShopWebPart.manifest.json"
        }
      ]
    },
    "simple-shop-cart-web-part": {
